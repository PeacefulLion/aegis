{
  "name": "wardmonitor",
  "version": "0.0.4",
  "description": "基于 badjs",
  "main": "index.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "start": "parcel ./src/*.html --public-url /aegis/ ",
    "clean": "rimraf dist/",
    "build": "cross-env NODE_ENV=production parcel build ./src/index.html --no-source-maps --public-url /aegis/ --detailed-report",
    "tar": "tar -cvf dist/wardmonitor.tar dist/",
    "release": "npm run clean && npm run build && npm run tar && echo '已打包: dist/wardmonitor.tar; rz 到服务器 tar -xvf 解压即可'"
  },
  "dependencies": {
    "@types/echarts": "^4.1.3",
    "antd": "^3.15.1",
    "axios": "^0.18.0",
    "dayjs": "^1.7.8",
    "echarts": "^4.2.0-rc.2",
    "eventbus": "0.0.3",
    "highcharts": "^7.0.2",
    "proptypes": "^1.1.0",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "react-highcharts": "^16.0.2",
    "react-infinite-scroller": "^1.2.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "source-map": "^0.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-highcharts": "^16.0.0",
    "@types/react-redux": "^6.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "conventional-changelog-cli": "^2.0.21",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-config-ivweb": "^0.1.5",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-react-hooks": "0.0.0",
    "koa": "^2.6.2",
    "less": "^3.9.0",
    "parcel": "^1.11.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iv-web/wardmonitor.git"
  },
  "author": "iv-web",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iv-web/wardmonitor/issues"
  },
  "homepage": "https://github.com/iv-web/wardmonitor#readme"
}
